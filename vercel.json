{
  "version": 2,
  "name": "luminus",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/webhooks",
      "dest": "/api/webhooks",
      "methods": ["POST"],
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app/api/webhooks/route.ts": {
      "maxDuration": 30
    }
  }
}
